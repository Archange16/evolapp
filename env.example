# Database Supabase
# Récupérez cette URL depuis votre projet Supabase > Settings > Database > Connection string > URI
DATABASE_URL="postgresql://postgres:[VOTRE-MOT-DE-PASSE]@db.[PROJECT-REF].supabase.co:5432/postgres"

# Email (optionnel - pour continuer à envoyer des emails en plus de la sauvegarde en BDD)
SMTP_USER="votre-email@gmail.com"
SMTP_PASS="votre-mot-de-passe-app"

# Resend (optionnel - pour la newsletter)
RESEND_API_KEY="votre-clé-resend"

